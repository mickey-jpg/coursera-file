                             function J = computeCost(X, y, theta)
% h = X*theta;
% temp = 0;
% for i=1:m
% temp = temp + (h(i) - y(i))^2;
% end
% J = (1/(2*m)) * temp;
end
function =(computecostmulti.m)
m = length(y); % number of training examples
% You need to return the following variables correctly
 J = 0;
 J = (1/(2*m))*(sum(((X*theta)-y).^2));
%
 =========================================================================
end 